{
    "version": 2,
    "builds": [
      {
        "src": "api/notifyer.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "api/notifyer.py"
      },
      {
        "src": "/api/telegram",
        "dest": "api/notifyer.py"
      }
    ]
  }
  